{
	"name": "gutenberg-starter",
	"version": "1.0.0",
	"description": "",
	"main": "build/index.js",
	"scripts": {
		"build-scss": "postcss assets/scss/main.scss -o dist/css/main.css",
		"watch-scss": "postcss assets/scss/main.scss -o dist/css/main.css --watch",
		"build-js": "babel assets/js --out-dir dist/js",
		"watch-js": "babel assets/js --out-dir dist/js --watch",
		"build-assets": "npm run build-scss && npm run build-js",
		"watch-assets": "npm run watch-scss & npm run watch-js",
		"tailwindwatch": "tailwindcss -i ./src/*.js -o ./build/style.css --watch --postcss --minify",
		"test": "echo \"Error: no test specified\" && exit 1",
		"watch-blocks": "cross-env NODE_ENV=development SCRIPT_DEBUG=true wp-scripts start && tailwindwatch --experimental-modules",
		"start": "concurrently --kill-others-on-fail \"npm run watch-blocks\" \"npm run watch-assets\"",
		"block-dev": "concurrently --kill-others-on-fail \"npm run watch-blocks\"",
		"build": "wp-scripts build --experimental-modules",
		"start-modules": "wp-scripts start --experimental-modules",
		"jest": "jest",
		"jest:watch": "jest --watch",
		"jest:watchAll": "jest --watchAll",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js",
		"packages-update": "wp scripts packages-update"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@woocommerce/woocommerce-rest-api": "^1.0.1",
		"@wordpress/icons": "^10.3.0",
		"classname": "^0.0.0",
		"classnames": "^2.5.1",
		"react-error-boundary": "^5.0.0",
		"stream": "^0.0.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.26.4",
		"@babel/core": "^7.26.7",
		"@babel/node": "^7.26.0",
		"@babel/plugin-transform-runtime": "^7.25.9",
		"@babel/preset-env": "^7.26.7",
		"@babel/preset-react": "^7.26.3",
		"@babel/register": "^7.25.9",
		"@babel/runtime": "^7.26.10",
		"@emotion/jest": "^11.13.0",
		"@playwright/test": "^1.54.2",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.2.0",
		"@wordpress/babel-plugin-makepot": "^6.17.0",
		"@wordpress/babel-preset-default": "^8.5.0",
		"@wordpress/interactivity": "^6.5.0",
		"@wordpress/jest-preset-default": "^12.18.0",
		"@wordpress/scripts": "^30.12.0",
		"autoprefixer": "^10.4.21",
		"babel-jest": "^29.7.0",
		"concurrently": "^9.1.2",
		"cross-env": "^7.0.3",
		"cypress": "^13.11.0",
		"dotenv": "^17.2.1",
		"jest": "^29.7.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.5.3",
		"postcss-cli": "^11.0.1",
		"postcss-import": "^16.1.0",
		"postcss-nested": "^7.0.2",
		"styled-components": "^6.1.15",
		"tailwindcss": "^3.4.17"
	}
}
