{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "anam-gutenberg-starter-block/smart-category-posts",
	"version": "0.1.0",
	"title": "Smart Category Posts",
	"category": "anam-starter",
	"icon": "media-interactive",
	"description": "",
	"example": {},
	"supports": {
		"interactivity": false
	},
	"attributes": {
		"label": {
			"type": "string",
			"default": "hello from label"
		},
		"postId": {
			"type": "number",
			"default": null
		},
		"categories": {
			"type": "array",
			"default": []
		},
		"fetchedPosts": {
			"type": "array",
			"default": []
		},
		"showExcerpt": {
			"type": "boolean",
			"default": true
		},
		"showCategory": {
			"type": "boolean",
			"default": true
		},
		"showFeaturedImage": {
			"type": "boolean",
			"default": true
		}
	},
	"textdomain": "gutenberg-starter",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": ["file:./view.js"]
}
