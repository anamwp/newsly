{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "newsly-block/post-lists-tab",
  "version": "0.1.0",
  "title": "Post Lists Tab",
  "category": "newsly",
  "icon": "media-interactive",
  "description": "",
  "example": {},
  "supports": {
    "interactivity": false
  },
  "attributes": {
    "label": {
      "type": "string",
      "default": "hello from label"
    },
    "postId": {
      "type": "number",
      "default": null
    },
    "categories": {
      "type": "array",
      "default": []
    },
    "fetchedPosts": {
      "type": "array",
      "default": []
    },
    "showExcerpt": {
      "type": "boolean",
      "default": true
    },
    "showCategory": {
      "type": "boolean",
      "default": true
    },
    "showFeaturedImage": {
      "type": "boolean",
      "default": true
    }
  },
  "textdomain": "gutenberg-starter",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": [
    "file:./view.js"
  ]
}