{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "anam-gutenberg-starter-block/movie-lists",
  "version": "0.1.0",
  "title": "Movie Lists",
  "category": "anam-starter",
  "icon": "media-interactive",
  "description": "",
  "example": {},
  "attributes": {
    "postId": {
      "type": "number",
      "default": null
    },
    "genres": {
      "type": "array",
      "default": []
    },
    "selectedCategroyId": {
      "type": "string",
      "default": ""
    },
    "selectedPostId": {
      "type": "string",
      "default": ""
    },
    "selectedCategoryPosts": {
      "type": "array",
      "default": []
    },
    "fetchedMovies": {
      "type": "array",
      "default": []
    },
    "movieColumn": {
      "type": "number",
      "default": 4
    },
    "showGenre": {
      "type": "boolean",
      "default": true
    },
    "showLanguage": {
      "type": "boolean",
      "default": true
    },
    "showReleaseDate": {
      "type": "boolean",
      "default": true
    },
    "showVoteCount": {
      "type": "boolean",
      "default": true
    },
    "showVoteAverage": {
      "type": "boolean",
      "default": true
    },
    "titleFontWeight": {
      "type": "string",
      "default": "400"
    },
    "titleFontSize": {
      "type": "number",
      "default": 20
    },
    "titleLetterSpacing": {
      "type": "number",
      "default": 0
    },
    "titleLineHeight": {
      "type": "number",
      "default": 1.5
    },
    "titleStyle": {
      "type": "string",
      "default": "normal"
    },
    "titleDecoration": {
      "type": "string",
      "default": "none"
    },
    "titleTransform": {
      "type": "string",
      "default": "none"
    },
    "titlePaddingAttr": {
      "type": "object",
      "default": {
        "top": "0px",
        "right": "0px",
        "bottom": "0px",
        "left": "0px"
      }
    },
    "contentPaddingAttr": {
      "type": "object",
      "default": {
        "top": "0px",
        "right": "0px",
        "bottom": "0px",
        "left": "0px"
      }
    }
  },
  "textdomain": "gutenberg-starter",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}